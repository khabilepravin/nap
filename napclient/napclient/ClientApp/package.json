{
  "name": "spark-react",
  "version": "3.3.0",
  "description": "Spark - Admin & Dashboard Template [ReactJS]",
  "author": "Paul Laros <support@bootlab.io>",
  "private": true,
  "scripts": {
    "sass:compile": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/assets/scss/ -o public/css/",
    "sass:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "build:css": "npm-run-all sass:*",
    "build:js": "react-scripts build",
    "watch:css": "chokidar \"src/assets/scss/**/*\" -c \"npm run build:css\"",
    "watch:js": "react-scripts start",
    "start": "npm-run-all -p build:css watch:*",
    "build": "npm-run-all -p build:*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "> 0.25%",
    "ie >= 11",
    "not op_mini all",
    "not dead"
  ],
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@auth0/auth0-react": "^1.0.0",
    "@auth0/auth0-spa-js": "^1.10.0",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/free-regular-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/react-fontawesome": "0.1.7",
    "apexcharts": "3.10.1",
    "apollo-boost": "^0.4.7",
    "availity-reactstrap-validation": "2.6.0",
    "axios": "^0.19.2",
    "bootstrap": "4.3.1",
    "chart.js": "2.9.2",
    "chokidar-cli": "2.1.0",
    "classnames": "2.2.6",
    "fullcalendar": "3.10.1",
    "google-map-react": "1.1.5",
    "graphql": "^15.0.0",
    "ionicons": "4.6.3",
    "jquery": "3.4.1",
    "jvectormap-next": "3.0.0",
    "moment": "2.24.0",
    "node-sass": "4.13.0",
    "node-sass-chokidar": "1.3.5",
    "npm-run-all": "4.1.5",
    "postcss-cli": "6.1.3",
    "react": "^16.13.1",
    "react-apexcharts": "1.3.3",
    "react-app-polyfill": "1.0.4",
    "react-bootstrap-table-next": "3.2.1",
    "react-bootstrap-table2-paginator": "2.1.0",
    "react-bootstrap-table2-toolkit": "2.1.0",
    "react-chartjs-2": "2.8.0",
    "react-datetime": "2.16.3",
    "react-dom": "^16.13.1",
    "react-dragula": "1.1.17",
    "react-feather": "2.0.3",
    "react-hook-form": "^5.7.1",
    "react-input-mask": "2.0.4",
    "react-ionicons": "3.0.1",
    "react-popper": "1.3.4",
    "react-quill": "1.3.3",
    "react-redux": "7.1.1",
    "react-redux-toastr": "7.6.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.2.0",
    "react-select": "3.0.8",
    "react-transition-group": "4.3.0",
    "reactstrap": "8.1.1",
